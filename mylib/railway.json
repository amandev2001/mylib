{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "DOCKERFILE",
    "dockerfilePath": "/mylib/Dockerfile"
  },
  "deploy": {
    "startCommand": "java -jar target/*.jar",
    "healthcheckEnabled": false,
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 10,
    "numReplicas": 1
  }
}
